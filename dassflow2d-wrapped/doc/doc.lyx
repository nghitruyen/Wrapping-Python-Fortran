\begin{equation}
\left\{ \begin{array}{ccc}
h_{K_{e}}^{G} & = & h_{\text{user}}\ \text{or}\ f_{\text{user}}(Q_{out})\\
\hat{u}_{K_{e}}^{G} & = & \hat{u}_{K}+2\left(\sqrt{g\left(h_{K}-h_{K}^{G}\right)}\right)\\
\hat{v}_{K_{e}}^{G} & = & \hat{v}_{K}
